{
    "name": "微信公众号编辑助手",
    "description": "weixin public platform editor",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "activeTab",
        "cookies",
        "storage",
        "webNavigation",
        "clipboardRead",
        "clipboardWrite",
        "\u003Call_urls\u003E"
    ],
    "browser_action": {
        "default_icon": "./images/wechat.png"
    },
    "icons": {
        "128": "./images/wechat.png"
    },
    "content_scripts": [{
        "run_at": "document_end",
        "matches": ["https://mp.weixin.qq.com/cgi-bin/appmsg?t=media/appmsg_edit*"],
        "js": ["./inject.js", "./weekly.inject.js"]
    }, {
        "run_at": "document_end",
        "matches": ["https://mp.weixin.qq.com/cgi-bin/*"],
        "js": ["./wxtoken.js"]
    }],
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./index.html",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}